        "cron": "^1.2.1",
        "dotenv": "^4.0.0",
        "email-existence": "^0.1.5",
        "eslint": "^3.18.0",
        "express": "^4.13.3",
        "express-jwt": "^5.3.0",
        "express-validator": "^3.2.0",
        "google-drive": "0.0.7",
        "googleapis": "^19.0.0",
        "in_array": "^1.1.0",
        "mailparser": "^2.0.5",
        "mandrill-api": "^1.0.45",
        "moment": "^2.18.1",
        "mongoose": "^4.9.8",
        "morgan": "^1.8.0",
        "mysql": "^2.13.0",
        "newman": "^3.5.2",
